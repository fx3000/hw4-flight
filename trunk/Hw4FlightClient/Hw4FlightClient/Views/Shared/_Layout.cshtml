<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    
    <link href="../../Contents/Styles/muatocroistyle.css" rel="stylesheet" type="text/css" d="underthesea-style-css"/>
    <link href="../../Contents/Styles/gocsuytustyle.css" rel="stylesheet" type="text/css" />
    <link href="../../Contents/Styles/socialstyle.css" rel="stylesheet" type="text/css" />

    <link rel="stylesheet" id="underthesea-portlligatsans-css" href="http://fonts.googleapis.com/css?family=Open+Sans%3A400%2C700&amp;subset=vietnamese&amp;ver=3.5-RC5" type="text/css" media="all">
    <link rel="stylesheet" id="underthesea-oswald-css" href="http://fonts.googleapis.com/css?family=Oswald%3A400%2C700%2C300&amp;ver=3.5-RC5" type="text/css" media="all">
    <link rel="stylesheet" id="underthesea-pacifico-css" href="http://fonts.googleapis.com/css?family=Pacifico&amp;ver=3.5-RC5" type="text/css" media="all">

    <link href="../../Contents/Styles/jquery-ui.css" rel="stylesheet" type="text/css" />
    <script src="../../Contents/Scripts/jquery-1.8.3.js" type="text/javascript"></script>
    <script src="../../Contents/Scripts/jquery-ui.js" type="text/javascript"></script>
    <script src="../../Contents/Scripts/jquery-ui-timepicker-addon.js" type="text/javascript"></script>
</head>
    <body>
        <header id="masthead" class="site-header" role="banner">
            <div>
                <h1 class="site-title"><a href="@Url.Action("Index", "Home")" title="Words" rel="home">Bay Rẻ</a></h1>
            </div>
            <nav role="navigation" class="site-navigation main-navigation">
                <h2 class="site-description">Rẻ nhất cho bạn</h2>
                @{ Html.RenderPartial("Partials/_TopMenuPartial"); }
            </nav>
        </header>
        
        <div id="main">
            <div id="primary" class="site-content">
                <div id="content" role="main">
                    @RenderBody()
                </div>
            </div>

            <div id="secondary" class="widget-area" role="complementary">
                <div class="widget">
                    <h1 class="widget-title">Đối tác</h1>	
                    <img src="../../Contents/Images/logoJetStart.png" />
                    <img src="../../Contents/Images/logoVietnamAirlines.gif" />
                    <img src="../../Contents/Images/logoAirMekong.jpg" />
                </div>
            </div>

            <div id="footer">
                @{Html.RenderPartial("Partials/_FooterPartial");}
            </div>
        </div>
    </body>
</html>

<head>
    
    
</head>
